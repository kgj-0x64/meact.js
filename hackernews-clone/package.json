{
  "name": "hackernews-clone",
  "version": "0.1.0",
  "description": "A Hacker News clone built to demonstrate Meact.js (React.js clone) for production ready websites.",
  "author": "Kumar Gaurav",
  "license": "MIT",
  "keywords": [
    "hacker-news",
    "clone",
    "meact",
    "react"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build-client": "tsx meact-framework/server-runtime/esbuild.config.js",
    "build-server": "tsx meact-framework/server-runtime/framework.build.ts",
    "build": "npm install && npm run build-client",
    "start": "tsx meact-framework/server-runtime/server.ts",
    "serve": "npm run build && npm run start",
    "ts:check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@firebase/app": "^0.10.9",
    "@firebase/database": "^1.0.7",
    "body-parser": "^1.20.2",
    "cookie": "^0.6.0",
    "debug": "^4.3.6",
    "express": "^4.19.2",
    "glob": "^11.0.0",
    "hackernews-clone": "file:",
    "lru-cache": "^11.0.0",
    "tsx": "^4.18.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/node": "^22.4.1",
    "esbuild": "^0.23.0",
    "typescript": "^5.5.4"
  }
}
